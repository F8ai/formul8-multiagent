{
  "version": 2,
  "builds": [
    {
      "src": "lambda.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["public/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/lambda.js"
    },
    {
      "src": "/health",
      "dest": "/lambda.js"
    },
    {
      "src": "/(.*)",
      "dest": "/lambda.js"
    }
  ]
}