{
  "version": 2,
  "builds": [
    {
      "src": "lambda.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/lambda.js"
    },
    {
      "src": "/health",
      "dest": "/lambda.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/lambda.js"
    }
  ],
  "outputDirectory": "public"
}