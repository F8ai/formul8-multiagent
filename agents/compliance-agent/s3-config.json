{
  "s3": {
    "enabled": true,
    "bucket": "formul8-platform-deployments",
    "prefix": "data/compliance",
    "region": "us-east-1",
    "cacheEnabled": true,
    "cacheTTL": 3600
  },
  "astradb": {
    "enabled": true,
    "endpoint": "${ASTRA_DB_API_ENDPOINT}",
    "token": "${ASTRA_DB_TOKEN}",
    "keyspace": "compliance_regulations",
    "collection": "regulations"
  },
  "dataAccess": {
    "preferredSource": "astradb",
    "fallbackToS3": true,
    "localCache": "/tmp/compliance-cache"
  },
  "jurisdictions": {
    "us_states": [
      "AL", "AK", "AZ", "AR", "CA", "CO", "CT", "DE", "FL", "GA",
      "HI", "ID", "IL", "IN", "IA", "KS", "KY", "LA", "ME", "MD",
      "MA", "MI", "MN", "MS", "MO", "MT", "NE", "NV", "NH", "NJ",
      "NM", "NY", "NC", "ND", "OH", "OK", "OR", "PA", "RI", "SC",
      "SD", "TN", "TX", "UT", "VT", "VA", "WA", "WV", "WI", "WY", "DC"
    ],
    "dataAvailable": [
      "CA", "CO", "FL", "HI", "ID", "IN", "MI", "MO", "NJ", "NY"
    ]
  }
}

