{
  "langchain": {
    "tier": "enterprise",
    "version": "0.1.0",
    "description": "Enterprise tier LangChain configuration with full capabilities and white-label options",
    "providers": {
      "openrouter": {
        "apiKey": "${OPENROUTER_API_KEY}",
        "baseURL": "https://openrouter.ai/api/v1",
        "defaultHeaders": {
          "HTTP-Referer": "https://f8.syzygyx.com",
          "X-Title": "Formul8 Multiagent Chat - Enterprise"
        }
      },
      "openai": {
        "apiKey": "${OPENAI_API_KEY}",
        "baseURL": "https://api.openai.com/v1",
        "defaultHeaders": {
          "OpenAI-Organization": "${OPENAI_ORG_ID}"
        }
      }
    },
    "models": {
      "routing": {
        "model": "meta-llama/llama-3.1-70b-instruct",
        "temperature": 0.3,
        "maxTokens": 50,
        "timeout": 5000
      },
      "agent_response": {
        "model": "meta-llama/llama-3.1-70b-instruct",
        "temperature": 0.7,
        "maxTokens": 2000,
        "timeout": 45000
      },
      "compliance": {
        "model": "meta-llama/llama-3.1-70b-instruct",
        "temperature": 0.4,
        "maxTokens": 3000,
        "timeout": 60000
      },
      "formulation": {
        "model": "meta-llama/llama-3.1-70b-instruct",
        "temperature": 0.5,
        "maxTokens": 3000,
        "timeout": 60000
      },
      "operations": {
        "model": "meta-llama/llama-3.1-70b-instruct",
        "temperature": 0.6,
        "maxTokens": 2000,
        "timeout": 45000
      },
      "premium": {
        "model": "openai/gpt-4o",
        "temperature": 0.7,
        "maxTokens": 4000,
        "timeout": 60000
      }
    },
    "chains": {
      "routing": {
        "type": "prompt_chain",
        "enabled": true,
        "fallback_enabled": true
      },
      "agent_response": {
        "type": "prompt_chain",
        "enabled": true
      },
      "compliance": {
        "type": "prompt_chain",
        "enabled": true,
        "specialized": true
      },
      "formulation": {
        "type": "prompt_chain",
        "enabled": true,
        "specialized": true
      },
      "operations": {
        "type": "prompt_chain",
        "enabled": true,
        "specialized": true
      },
      "multi_agent": {
        "type": "sequential_chain",
        "enabled": true,
        "max_agents": 5
      },
      "parallel_processing": {
        "type": "parallel_chain",
        "enabled": true,
        "max_concurrent": 3
      }
    },
    "prompts": {
      "routing": {
        "template": "You are an intelligent agent router for the Formul8 cannabis industry AI system. Route user messages to the most appropriate specialized agent.\n\nAvailable agents:\n{agents_list}\n\nUser message: {message}\n\nAnalyze the message and respond with ONLY the agent name.",
        "inputVariables": ["agents_list", "message"]
      },
      "agent_system": {
        "template": "You are the {agent_name}, a {agent_description} specializing in:\n{agent_specialties}\n\nProvide detailed, accurate, and professional responses about cannabis industry topics.\nAlways cite specific regulations when possible and recommend consulting with legal professionals for complex matters.\n\nUser question: {message}\n\nProvide a comprehensive response:",
        "inputVariables": ["agent_name", "agent_description", "agent_specialties", "message"]
      },
      "white_label": {
        "template": "You are the {agent_name} for {company_name}. {agent_description}\n\n{agent_specialties}\n\nUser question: {message}\n\nProvide a response that reflects {company_name}'s brand and values:",
        "inputVariables": ["agent_name", "company_name", "agent_description", "agent_specialties", "message"]
      }
    },
    "memory": {
      "enabled": true,
      "type": "conversation_buffer",
      "maxTokens": 5000
    },
    "tools": {
      "enabled": true,
      "available": ["calculator", "web_search", "code_interpreter", "file_reader", "api_caller", "database_query"]
    },
    "callbacks": {
      "enabled": true,
      "types": ["console_logging", "performance_monitoring", "usage_tracking", "analytics", "custom_callbacks"]
    },
    "rate_limiting": {
      "enabled": false
    },
    "features": {
      "advanced_routing": true,
      "conversation_memory": true,
      "tool_integration": true,
      "custom_prompts": true,
      "analytics": true,
      "formulation_tools": true,
      "compliance_checking": true,
      "marketing_analysis": true,
      "operations_management": true,
      "sourcing_optimization": true,
      "patent_research": true,
      "spectral_analysis": true,
      "customer_success": true,
      "f8_slackbot": true,
      "mcr_management": true,
      "ad_strategy": true,
      "multi_agent_workflows": true,
      "parallel_processing": true,
      "white_label_solutions": true,
      "custom_workflow_integrations": true,
      "advanced_analytics": true,
      "multi_user_management": true,
      "api_access": true,
      "custom_training": true,
      "sla_guarantees": true,
      "dedicated_support": true,
      "unlimited_everything": true
    },
    "white_label": {
      "enabled": true,
      "custom_branding": true,
      "custom_domain": true,
      "custom_ui": true,
      "custom_prompts": true
    },
    "api_access": {
      "enabled": true,
      "rate_limits": "unlimited",
      "endpoints": ["chat", "agents", "health", "analytics", "admin"]
    }
  }
}