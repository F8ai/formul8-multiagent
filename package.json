{
  "name": "formul8-multiagent-tests",
  "version": "1.0.0",
  "description": "Comprehensive Playwright tests for F8 Multiagent System",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:validate": "node tests/validate-system.js",
    "test:chatgpt": "node tests/run-chatgpt-comparison.js",
    "test:chatgpt:visual": "playwright test tests/visual-chatgpt-comparison.spec.js",
    "test:chatgpt:functionality": "playwright test tests/chatgpt-functionality-mirror.spec.js",
    "test:chatgpt:comparison": "playwright test tests/chatgpt-comparison.spec.js",
    "deploy:vercel": "vercel --prod",
    "deploy:protected": "./deploy-protected.sh",
    "dev:vercel": "vercel dev"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "dotenv": "^16.3.1"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}