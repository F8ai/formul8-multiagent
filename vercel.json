{
  "version": 2,
  "builds": [
    {
      "src": "lambda-package/lambda.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/lambda-package/lambda.js"
    },
    {
      "src": "/health",
      "dest": "/lambda-package/lambda.js"
    },
    {
      "src": "/chat$",
      "dest": "/public/chat.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
  }
}
